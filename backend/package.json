{
  "name": "snw-backend",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build:server": "tsc -p tsconfig.build.json",
    "build:watch": "onchange \"src/**/*.ts\" -- npm run build",
    "build:bundle": "node ./scripts/create-production-bundle.js",
    "start:bundle": "cross-env NODE_ENV=production node build/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "test": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --coverage --watchAll --detectOpenHandles --runInBand",
    "test:single": "cross-env NODE_ENV=test jest --verbose -t",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "RD /S /Q coverage src\\backend\\build",
    "lint": "eslint --ext .ts",
    "jest:clear_cache": "jest --clearCache"
  },
  "author": "Samu Linden",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/google-spreadsheet": "^3.0.0",
    "@types/ioredis": "^4.16.6",
    "@types/jest": "^25.2.1",
    "@types/marked": "^0.7.4",
    "@types/morgan": "^1.9.1",
    "@types/node": "^13.13.2",
    "@types/node-cron": "^2.0.3",
    "@types/source-map-support": "^0.5.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "jest": "^25.4.0",
    "onchange": "^6.1.1",
    "serve": "^11.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-spreadsheet": "^3.1.15",
    "ioredis": "^4.17.3",
    "marked": "^4.0.10",
    "morgan": "^1.10.0",
    "node-cron": "^2.0.3",
    "source-map-support": "^0.5.18"
  }
}
