{
  "name": "snw-backend",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "onchange \"src/**/*.ts\" -- npm run build",
    "start": "cross-env NODE_ENV=production node src/build/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "RD /S /Q coverage src\\build",
    "lint": "eslint --ext .ts",
    "ga": "ts-node src/services/googleSheetsService.ts"
  },
  "author": "Samu Linden",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/source-map-support": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "jest": "^25.4.0",
    "onchange": "^6.1.1",
    "serve": "^11.3.0",
    "source-map-support": "^0.5.18",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "google-spreadsheet": "^3.0.11"
  }
